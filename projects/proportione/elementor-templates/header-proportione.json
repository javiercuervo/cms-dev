        "settings": {
          "menu": "menu-principal",
          "layout": "horizontal",
          "align_items": "center",
          "pointer": "underline",
          "animation_line": "fade",
          "submenu_icon": {"value": "fas fa-angle-down", "library": "fa-solid"},
