[
  {
    "id": "header-main",
    "elType": "container",
    "settings": {
      "content_width": "full",
      "flex_direction": "row",
      "flex_wrap": "nowrap",
      "flex_align_items": "center",
      "flex_justify_content": "space-between",
      "min_height": {"unit": "px", "size": 70},
      "min_height_tablet": {"unit": "px", "size": 65},
      "min_height_mobile": {"unit": "px", "size": 60},
      "padding": {"unit": "px", "top": "10", "right": "40", "bottom": "10", "left": "40"},
